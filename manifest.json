{
    "manifest_version": 3,
    "name": "Performoo Debugger",
    "description": "Base Level Extension",
    "version": "1.0",
    "action": {
      "default_popup": "popup.html"
    },
    "content_scripts": [
        {
          "matches": ["<all_urls>"],
          "js": ["content.js"]
        }
    ],
    "icons": {
        "128": "128.png",
        "48": "128.png",
        "16": "128.png"
      },
      "browser_action": {
        "default_icon": {
          "16": "128.png"
        }
      }
  }